MODEL_NAME=foo
LISTNAME=eval
FASTEVAL_FILES=$(sort $(wildcard data/$(MODEL_NAME)/tessdata_fast/*_[0-2].[0-9]*.traineddata))
DUMMYLOG=$(subst tessdata_fast,tessdata_fast,$(patsubst %.traineddata,%.dummyfile,$(FASTEVAL_FILES)))

fasteval: $(DUMMYLOG)
.PHONY: fasteval

$(DUMMYLOG): %.dummyfile: %.traineddata
	echo "***** $< ****"
	OMP_THREAD_LIMIT=1 lstmeval  \
    --verbosity=0 \
    --model $< \
    --eval_listfile data/$(MODEL_NAME)/list.${LISTNAME}  2>&1  $@
